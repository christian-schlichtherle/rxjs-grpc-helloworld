{
  "name": "rxjs-grpc-hello-world",
  "description": "A simple example for rxjs-grpc with streaming response.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kondi/rxjs-grpc.git"
  },
  "scripts": {
    "start": "npm run grpc-namespaces",
    "server": "ts-node server",
    "client": "ts-node client",
    "preserver": "npm run grpc-namespaces",
    "preclient": "npm run grpc-namespaces",
    "grpc-namespaces": "rxjs-grpc -o grpc-namespaces.ts *.proto"
  },
  "dependencies": {
    "grpc": "^1.18.0",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "rxjs-grpc": "^0.1.8",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  }
}
